[
  {
    "name": "skitai-app",
    "image": "registry.gitlab.com/skitai/ecsdep:latest",
    "repositoryCredentials": {
      "credentialsParameter": "arn:aws:secretsmanager:ap-northeast-1:689724919193:secret:gitlab/registry/hansroh-PrENMF"
    },
    "essential": true,
    "mountPoints": [],
    "environment": [
      {
        "name": "SERVICE_STAGE",
        "value": "qa"
      }
    ],
    "secrets": [
      {
        "name": "REGISTRY_USER",
        "valueFrom": "arn:aws:secretsmanager:ap-northeast-1:689724919193:secret:gitlab/registry/hansroh-PrENMF:username::"
      }
    ],
    "memoryReservation": 160,
    "memory": 512,
    "cpu": 1024,
    "healthCheck": {
      "command": [
        "CMD-SHELL",
        "wget -O/dev/null -q http://localhost:5000 || exit 1"
      ],
      "retries": 3,
      "interval": 30
    },
    "dependsOn": [],
    "portMappings": [
      {
        "containerPort": 5000,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/skitai-cluster/ecsdep-qa/skitai-app",
        "awslogs-stream-prefix": "ecs",
        "awslogs-region": "ap-northeast-1"
      }
    }
  },
  {
    "name": "skitai-nginx",
    "image": "registry.gitlab.com/skitai/ecsdep/nginx:latest",
    "repositoryCredentials": {
      "credentialsParameter": "arn:aws:secretsmanager:ap-northeast-1:689724919193:secret:gitlab/registry/hansroh-PrENMF"
    },
    "essential": false,
    "mountPoints": [],
    "environment": [
      {
        "name": "SERVICE_STAGE",
        "value": "qa"
      }
    ],
    "secrets": [],
    "memoryReservation": 16,
    "cpu": 0,
    "dependsOn": [
      {
        "containerName": "skitai-app",
        "condition": "HEALTHY"
      }
    ],
    "links": [
      "skitai-app"
    ],
    "portMappings": [
      {
        "containerPort": 80,
        "hostPort": 0,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/skitai-cluster/ecsdep-qa/skitai-nginx",
        "awslogs-stream-prefix": "ecs",
        "awslogs-region": "ap-northeast-1"
      }
    }
  }
]